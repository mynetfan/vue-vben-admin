{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Vben Playground",
  "version": "0.1.0",
  "identifier": "pro.vben.playground",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5555",
    "beforeDevCommand": "pnpm dev:web",
    "beforeBuildCommand": "pnpm build:web"
  },
  "app": {
    "windows": [
      {
        "title": "Vben Playground",
        "width": 1440,
        "height": 900,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "windows": {
      "webviewInstallMode": {
        "silent": true,
        "type": "offlineInstaller"
      }
    },
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
